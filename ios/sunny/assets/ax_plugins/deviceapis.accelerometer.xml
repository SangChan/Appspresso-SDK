<?xml version="1.0" encoding="UTF-8"?>
<axplugin id="deviceapis.accelerometer" version="1.1.2">
	<description>WAC/Waikiki API Accelerometer Feature Appspresso Plugin
	</description>
	<url>http://appspresso.com</url>
	<author>Appspresso Dev. Team</author>
	<license>Copyright (c) 2011, KT Hitel Co., LTD.
	</license>

	<feature id="http://wacapps.net/api/accelerometer" category="Standard" />

	<module platform="android" platform-version="8"
		min-platform-version="7" max-platform-version=""
		class="com.appspresso.waikiki.accelerometer.Accelerometer">
	</module>

	<module platform="ios" platform-version="4.1"
		min-platform-version="4.0" max-platform-version=""
		class="KthWaikikiAccelerometer">
		<property name="framework" value="CoreMotion.framework" />
	</module>
</axplugin>